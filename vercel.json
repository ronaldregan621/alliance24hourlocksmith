{
  "cleanUrls": true,
  "trailingSlash": true,
  "redirects": [
    { "source": "/pricing", "destination": "/price-list/", "permanent": true },
    { "source": "/pricing/", "destination": "/price-list/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "no-referrer-when-downgrade" }
      ]
    }
  ]
} 